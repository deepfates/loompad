{
  "name": "spiel-bun",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec 'bun run.ts' --watch server --watch shared --watch *.ts --ext ts,tsx,js,mjs,json",
    "prebuild": "node scripts/build-icons.cjs",
    "build": "vite build --outDir=dist/server --ssr server/ssr.tsx  --config config/vite.config.ts && vite build --outDir=dist/client --config config/vite.config.ts",
    "prod": "bun run.ts --mode=production",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "test": "bun test"
  },
  "dependencies": {
    "@ax-llm/ax": "^14.0.39",
    "@types/d3-flextree": "^2.1.4",
    "@types/express": "^4.17.21",
    "@types/node": "^24.6.2",
    "@types/styled-components": "^5.1.34",
    "babel-plugin-styled-components": "^2.1.4",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "d3-flextree": "^2.1.2",
    "d3-hierarchy": "^3.1.2",
    "express": "^4.19.2",
    "nocache": "^4.0.0",
    "nodemon": "^3.1.0",
    "openai": "^4.77.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-components": "^6.1.8",
    "wouter": "^3.3.5",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@rollup/rollup-linux-x64-gnu": "^4.52.4",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "typescript": "^5.2.2",
    "vite": "^5.2.8",
    "vite-plugin-pwa": "^1.0.1"
  }
}
